        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataGridView.view"
        mc:Ignorable="d"
        Title="Contenedor" Width="1080" Height="720" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Viewer:CrystalReportsViewer Name="visor" />
        <Button Name="btnVolver" Grid.Column="1" Content="Volver" Width="150" Height="50" Click="btnVolver_Click"/>
    </Grid>
</Window>
